<h1>Actor #<%= @actor.id %></h1>
<div>Name: <%= @actor.name %></div>
<div><img src="<%= @actor.image_url %>"</div>
<div>DOB: <%= @actor.dob %></div>
<div>Bio: <%= @actor.bio %></div>
<a class="btn btn-default" href="/actors/">Go Back</a>
<a class="btn btn-default" href="/actors/<%= @actor.id %>/edit">Edit</a>
<a class="btn btn-default" href="/delete_actor/<%= @actor.id %>">Delete</a>
